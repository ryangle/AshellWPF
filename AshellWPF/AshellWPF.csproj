<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\WPFGalleryPreviewIcon.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Assets\WPFGalleryPreviewIcon.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Prism.DryIoc" Version="9.0.537" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Modules\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AshellWPF.Core\AshellWPF.Core.csproj" />
    <ProjectReference Include="..\AshellWPFModules\ControlsModule\ControlSamples.csproj" />
  </ItemGroup>

</Project>
